{
  "version": 2,
  "builds": [
    {
      "src": "dist/production.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/production.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "dist/production.js": {
      "maxDuration": 30
    }
  },
  "buildCommand": "npm run vercel-build"
}
